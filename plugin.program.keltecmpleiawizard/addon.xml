<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.keltecmpleiawizard" name="KelTecMP Leia Wizard Builds" version="0.0.5" provider-name="Team Ezequiel KeltecMP">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">KelTecMP Builds</summary>
    <description lang="en">Instalador KelTecMP Wizard, Para Todas as Versões do Kodi, não aceite Imitações.</description>
    <platform>all</platform>
  </extension>
</addon>